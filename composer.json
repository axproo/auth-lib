{
  "name": "axproo/auth-lib",
  "description": "Bibliothèque d'authentification complète et réutilisable pour CodeIgniter 4.",
  "type": "library",
  "version": "1.0.1",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Djomou",
      "email": "contact@axproo.com",
      "homepage": "https://axproo.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "firebase/php-jwt": "^6.10",
    "codeigniter4/framework": "^4.5",
    "ramsey/uuid": "^4.9",
    "axproo/mailer": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Axproo\\Auth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Axproo\\Auth\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4"
  }
}
